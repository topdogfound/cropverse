generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Crop {
  id Int @default(autoincrement()) @id
  name String
  description String
  category Category
}
enum Category {
  VEGETABLE
  FRUIT
  GRAIN
  LEGUME
  NUT
  HERB
  SPICE
}